{
    "Name": "fvnkhead.mod",
    "Description": "fvnkhead's server mod",
    "Version": "1.0.0",
    "LoadPriority": 1,
    "ConVars": [
        {
            "Name": "fm_debug_enabled",
            "DefaultValue": "1"
        },

        {
            "Name": "fm_admin_uids",
            "DefaultValue": "2441795736,999"
        },
        {
            "Name": "fm_admin_auth_enabled",
            "DefaultValue": "1"
        },
        {
            "Name": "fm_admin_password",
            "DefaultValue": "1234"
        },

        {
            "Name": "fm_welcome_enabled",
            "DefaultValue": "1"
        },
        {
            "Name": "fm_welcome",
            "DefaultValue": "welcome! see !help for available commands"
        },

        {
            "Name": "fm_rules_enabled",
            "DefaultValue": "1"
        },
        {
            "Name": "fm_rules_ok",
            "DefaultValue": "light banter, teasing, funny macros"
        },
        {
            "Name": "fm_rules_not_ok",
            "DefaultValue": "malicious speech, cheating, competitive macros"
        },

        {
            "Name": "fm_custom_commands_enabled",
            "DefaultValue": "1"
        },
        {
            "Name": "fm_custom_commands",
            "DefaultValue": "!discord=discord.gg/u76mkkEUkD;!weapons=banned weapons: smgs, r201, r101, flatline, spitfire"
        },

        {
            "Name": "fm_kick_enabled",
            "DefaultValue": "1"
        },
        {
            "Name": "fm_kick_save",
            "DefaultValue": "1"
        },
        {
            "Name": "fm_kick_percentage",
            "DefaultValue": "0.5"
        },
        {
            "Name": "fm_kick_min_players",
            "DefaultValue": "6"
        },

        {
            "Name": "fm_maps_enabled",
            "DefaultValue": "1"
        },
        {
            "Name": "fm_maps",
            "DefaultValue": "mp_lf_stacks, mp_lf_deck, mp_lf_uma"
        },
        {
            "Name": "fm_nextmap_enabled",
            "DefaultValue": "1"
        },

        {
            "Name": "fm_balance_enabled",
            "DefaultValue": "1"
        },
        {
            "Name": "fm_balance_percentage",
            "DefaultValue": "0.25"
        }
    ],
    "Scripts": [
        {
            "Path": "fm.nut",
            "RunOn": "SERVER && MP",
            "ServerCallback": {
                "After": "fm_Init"
            }
        }
    ]
}
